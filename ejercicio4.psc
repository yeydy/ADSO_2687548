Algoritmo calcularsalario 
	Definir años,sueldo,prom Como Entero
	Escribir "ingrese los años que lleva en la compañia ";
	leer años;
	si años<4 Entonces
		prom<-20;
	SiNo
		prom<-40;
		
	FinSi
	si 
		Escribir "ingrese su sueldo ";
		leer sueldo;
		sueldo<- (sueldo+sueldo)*prom;
		Escribir "su sueldo es " sueldo;
	FinSi
	
	
FinAlgoritmo
